<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nakamd.washington.edu.quizdroid"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:name="nakamd.washington.edu.quizdroid.QuizApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="nakamd.washington.edu.quizdroid.HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="nakamd.washington.edu.quizdroid.SubjectOverviewActivity"
            android:label="@string/title_activity_main_activity2" >
        </activity>
        <activity
            android:name="nakamd.washington.edu.quizdroid.QuestionActivity"
            android:label="@string/title_activity_question" >
        </activity>
        <activity
            android:name="nakamd.washington.edu.quizdroid.ResultActivity"
            android:label="@string/title_activity_result" >
        </activity>
        <activity
            android:name="nakamd.washington.edu.quizdroid.FinalActivity"
            android:label="@string/title_activity_final" >
        </activity>
    </application>

</manifest>